{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-param-reassign": 0,
        "linebreak-style": 0,
        "react/jsx-props-no-spreading": "off",
        "max-len": ["error", { "code": 120 }],
        "react/jsx-one-expression-per-line": "off"
    }
}
